{
  "name": "robo-thrall",
  "version": "1.0.0",
  "description": "discord bot",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "copy-config": "copyfiles -u 2 \"src/config/*.json\" dist/config/",
    "build": "tsc && npm run copy-config",
    "start": "node dist/index.js",
    "deploy": "node dist/deploy-commands.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-jace/robo-thrall.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/code-jace/robo-thrall/issues"
  },
  "homepage": "https://github.com/code-jace/robo-thrall#readme",
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
